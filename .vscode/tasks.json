{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/Fe:",
                "${workspaceFolder}\\build\\${fileBasenameNoExtension}.exe",
                "/I${workspaceFolder}\\include\\",
                "/I${workspaceFolder}\\",
                "${workspaceFolder}\\src\\**.cpp",
                "/link",
                "/LIBPATH:${workspaceFolder}\\libs",
                "sfml-graphics.lib",
                "sfml-window.lib",
                "sfml-system.lib",
                "sfml-audio.lib",
                "sfml-network.lib",
                "flac.lib",
                "freetype.lib",
                "ogg.lib",
                "openal32.lib",
                "vorbis.lib",
                "vorbisenc.lib",
                "vorbisfile.lib"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}